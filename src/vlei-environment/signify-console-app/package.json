{
  "name": "signify-console-app",
  "version": "1.0.0",
  "description": "TypeScript console application implementing signify-ts client setup and OOBI resolution",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js true",
    "dev": "tsc && node dist/main.js",
    "debug": "tsc && node --inspect-brk dist/main.js",
    "debug-watch": "tsc --watch & node --inspect-brk dist/main.js",
    "clean": "rimraf dist",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "signify-ts": "^0.3.0-rc1",
    "vlei-keria-library": "file:../../vlei-keria-proxy/vlei-keria-common"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "signify-ts",
    "keri",
    "typescript",
    "console"
  ],
  "author": "",
  "license": "MIT"
}
