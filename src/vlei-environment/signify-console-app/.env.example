# Signify Console App Configuration
LOCAL_PATH=YOUR_LOCAL_PATH_HERE

# KERIA Configuration
KERIA_ADMIN_URL=http://localhost:3901
KERIA_BOOT_URL=http://localhost:3903

# Schema Server Configuration  
